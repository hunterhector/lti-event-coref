      <name>edu.cmu.lti.event_coref.type.TemporalRelation</name>
      <description/>
      <supertypeName>edu.cmu.lti.util.type.ComponentTOP</supertypeName>
      <features>
        <featureDescription>
          <name>relation</name>
          <description>E.g., '=' in 't1=t2+5days'.</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>temporalVariableLhs</name>
          <description>E.g., t1 in 't1=t2+5days'.</description>
